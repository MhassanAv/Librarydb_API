{
  "name": "library-backend",
  "version": "0.1.0",
  "main": "app.ts",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "start": "node build/app.js",
    "watch": "tsc-watch --esModuleInterop src/app.ts --outDir ./build --onSuccess \"node ./build/app.js\"",
    "tsc": "tsc",
    "jasmine": "npx jasmine-ts",
    "test": "set ENV=test&&npx tsc&&npx db-migrate --env test up&&npx jasmine&&npx db-migrate --env test reset",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --config .prettierrc --write \"**/*.+(js|ts|json)\"",
    "format": "npm run lint && npm run prettier"
  },
  "author": "Muhammed Hassan",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "tsc-watch": "^4.2.9"
  },
  "keywords": []
}
